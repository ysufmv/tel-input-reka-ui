{
  "name": "tel-input-reka-ui",
  "version": "1.0.0",
  "description": "International Telephone Input for Vue 3 with Reka UI primitives",
  "author": "ysufmv",
  "license": "MIT",
  "homepage": "https://github.com/ysufmv/tel-input-reka-ui",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ysufmv/tel-input-reka-ui.git"
  },
  "exports": {
    ".": {
      "types": "./dist/tel-input.d.ts",
      "import": "./dist/tel-input.js",
      "require": "./dist/tel-input.umd.cjs"
    },
    "./style.css": "./dist/tel-input.css"
  },
  "main": "./dist/tel-input.umd.cjs",
  "module": "./dist/tel-input.js",
  "types": "./dist/tel-input.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest --ui",
    "build": "vue-tsc && vite build",
    "postbuild": "node -e \"require('fs').copyFileSync('src/types/index.d.ts', 'dist/tel-input.d.ts')\"",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "libphonenumber-js": "^1.10.0",
    "lucide-vue-next": ">=0.300.0",
    "reka-ui": ">=1.0.0",
    "vue": "^3.4.0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/ui": "^1.4.0",
    "@vue/test-utils": "^2.4.5",
    "jsdom": "^24.0.0",
    "libphonenumber-js": "^1.12.12",
    "lucide-vue-next": "^0.400.0",
    "reka-ui": "^2.0.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vitest": "^1.4.0",
    "vue": "^3.5.4",
    "vue-tsc": "^2.0.6"
  },
  "keywords": [
    "vue",
    "telephone",
    "phone",
    "tel-input",
    "phone input",
    "telephone input",
    "input",
    "international phone",
    "shadcn",
    "reka-ui",
    "shadcn-vue"
  ]
}
